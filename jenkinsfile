pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'mvn -f clean'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn -f test'
            }
        }
        stage('Deploy') {
            steps {
                sh 'mvn -f package'
            }
        }
    }
}
